<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.b07medicalapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.B07MedicalApp">
        <activity android:name=".Model"></activity>
        <activity android:name=".Presenter" />
        <activity
            android:name=".PatientInfo"
            android:exported="true" />
        <activity
            android:name=".InspectPatients"
            android:exported="true" />
        <activity
            android:name=".ListSchedule"
            android:exported="true" />
        <activity
            android:name=".ListAvailability"
            android:exported="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity">
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".ListAppointment"
            android:exported="true"
            android:label="@string/title_activity_list_appointment"
            android:theme="@style/Theme.B07MedicalApp.NoActionBar" />
        <activity android:name=".BookAppointmentActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>